<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#181818"
    tools:context=".Inicio">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/inicioMain"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="230dp"
            android:layout_height="200dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/boha_inicio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/btn_Comidas"
            android:layout_width="300dp"
            android:layout_height="140dp"
            android:layout_marginTop="50dp"
            android:backgroundTint="#181818"
            android:contentDescription="imgComidas"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:srcCompat="@drawable/comidas" />

        <TextView
            android:id="@+id/lblComidas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ver Comidas"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/btn_Comidas"
            app:layout_constraintStart_toStartOf="@+id/btn_Comidas"
            app:layout_constraintTop_toBottomOf="@+id/btn_Comidas" />

        <ImageButton
            android:id="@+id/btn_Bebidas"
            android:layout_width="300dp"
            android:layout_height="140dp"
            android:layout_marginTop="32dp"
            android:backgroundTint="#181818"
            android:contentDescription="imgBebidas"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lblComidas"
            app:srcCompat="@drawable/bebidas33" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ver Bebidas"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/btn_Bebidas"
            app:layout_constraintStart_toStartOf="@+id/btn_Bebidas"
            app:layout_constraintTop_toBottomOf="@+id/btn_Bebidas" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>